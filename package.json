{
  "name": "sira-ui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "chroma-js": "^2.4.2",
    "dayjs": "^1.11.8",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "eslint": "8.42.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "turbo": "^1.10.2",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.4"
}